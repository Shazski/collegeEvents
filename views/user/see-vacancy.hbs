    <title>Projects table by Gleb Varganov. </title>

            <script src="https://cdn.tailwindcss.com"></script>
    </head>
<body class="">
    
    <div class="overflow-x-auto">
        <div class="min-w-screen  bg-gray-100 flex items-center justify-center bg-gray-100 font-sans overflow-hidden">
            <div class="w-full lg:w-5/6">
                <div class="bg-white shadow-md rounded my-6">
                    <table class="min-w-max w-full table-auto">
                        <thead>
                            <tr class="bg-gray-200  uppercase text-sm leading-normal">
                                <th class="py-3 px-6 text-left">Team Name</th>
                                <th class="py-3 px-6 text-left">District</th>
                                <th class="py-3 px-6 text-center">Address</th>
                                <th class="py-3 px-6 text-center">Experience</th>
                                <th class="py-3 px-6 text-center">Contact No</th>
                                <th class="py-3 px-6 text-center">Day of work</th>
                                <th class="py-3 px-6 text-center">Apply</th>
                            </tr>
                        </thead>
                        <tbody class="text-gray-600 text-sm font-light">
                          {{#each vacancy}}
                            <tr class="border-b border-gray-200 hover:bg-gray-100">
                                <td class="py-3 px-6 text-left whitespace-nowrap">
                                    <div class="flex items-center">
                                        <div class="mr-2"> 
                                        </div>
                                        <span class="font-medium">{{this.teamName}}</span>
                                    </div>
                                </td>
                                <td class="py-3 px-6 text-left">
                                    <div class="flex items-center">
                                        
                                        <span>{{this.District}}</span>
                                    </div>
                                </td>
                                <td class="py-3 px-6 text-center">
                                   <span>{{this.address}}</span>
                                </td>
                                <td class="py-3 px-6 text-center">
                                    <span class="">{{this.experience}}</span>
                                </td>
                                <td class="py-3 px-6 text-center">
                                    <div class="flex item-center justify-center">
                                        <span>{{this.phone}}</span>
                                    </div>
                                </td>
                                <td class="py-3 px-6 text-center">
                                    <div class="flex item-center justify-center">
                                        <span>{{this.date}}</span>
                                    </div>
                                </td>
                                <td class="py-3 px-6 text-center">
                                    <div class="flex item-center justify-center">
                                        <button onclick="location='/apply?id={{this._id}}&event={{this.eventId}}'" class="btn btn-warning">Apply</button>
                                    </div>
                                </td>
                            </tr>
                            {{/each}}
                           
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
